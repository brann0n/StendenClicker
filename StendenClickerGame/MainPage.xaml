                                    <DataTemplate>
                                        <Button Margin="5,0,5,5" CornerRadius="5" Width="65">
                                            <TextBlock Foreground="White" TextTrimming="Clip"  Text="Jarno"/>
                                        </Button>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                                <ListView.ItemContainerStyle>
                                    <Style TargetType="ListViewItem">
                                        <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
                                        <Setter Property="Margin" Value="0"/>
                                        <Setter Property="Padding" Value="0"/>
                                        <Setter Property="MinWidth" Value="0"/>
                                    </Style>
                                </ListView.ItemContainerStyle>
                            </ListView>
                        </Grid>
                    </Border>
                </Grid>
            </Grid>
        </Grid>

        <!--Middle Grid Koffiemachine-->
        <Grid Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="89.5*"/>
                <RowDefinition Height="10.5*"/>
            </Grid.RowDefinitions>
            <!--Background koffiemachine met gradiant-->
            <Border CornerRadius="5" Margin="10,20,10,20">
                <Grid Grid.Row="0">
                    <Grid.Background>
                        <media:RadialGradientBrush MappingMode="RelativeToBoundingBox" InterpolationSpace="Auto">
                            <GradientStop Color="#6A6A6A" Offset="1"/>
                            <GradientStop Color="#A3A3A3" Offset="0"/>
                        </media:RadialGradientBrush>
                    </Grid.Background>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="18*"/>
                        <RowDefinition Height="82*"/>
                    </Grid.RowDefinitions>

                    <!--Tekst koffiemachine-->
                    <Grid Grid.Row="0">
                        <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Fonts/GFY_Michael_W00_Reg.ttf#GFYMichaelW00-Reg" FontSize="40" Foreground="White">Waar heeft u vandaag zin in?</TextBlock>
                    </Grid>

                    <!--List drankjes-->
                    <Grid Grid.Row="1" Name="KoffieGrid">
						<ListView Name="koffie" ItemsSource="{Binding KoffieMachine.AbilitiesList}"  Width="{Binding ActualWidth, ElementName=KoffieGrid}" Padding="0" UseLayoutRounding="False" Margin="0,10,0,0" SelectionMode="None">
                            <ListView.ItemTemplate>
                                <DataTemplate>
                                    <Button Foreground="White" Background="#424242"  BorderThickness="0" Margin="20,0,20,5" CornerRadius="5" Command="{Binding OnExecute}" IsEnabled="true">
                                        <StackPanel Width="{Binding ElementName=koffie, Path=Width}" Orientation="Horizontal" Height="68">
                                            <Grid Width="{Binding ElementName=koffie, Path=Width}" >
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="12*" />
                                                    <ColumnDefinition Width="88*" />
                                                </Grid.ColumnDefinitions>
                                                <Grid Grid.Column="0" >
                                                    <Image Source="{Binding Image}"/>
                                                </Grid>
                                                <Grid Grid.Column="1" Margin="10,0,40,0" HorizontalAlignment="left">
                                                    <StackPanel>
                                                        <TextBlock Foreground="White" Text="{Binding AbilitieName}" Grid.Column="0" RelativePanel.AlignHorizontalCenterWithPanel="True"  HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Fonts/GFY_Michael_W00_Reg.ttf#GFYMichaelW00-Reg" FontSize="30">De Tester</TextBlock>
                                                        <TextBlock Foreground="White" Text="{Binding AbilitieDescription}"/>
                                                    </StackPanel>
                                                </Grid>
                                            </Grid>
                                        </StackPanel>
                                    </Button>
                                </DataTemplate>
                            </ListView.ItemTemplate>
                            <ListView.ItemContainerStyle>
                                <Style TargetType="ListViewItem">
                                    <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                </Style>
                            </ListView.ItemContainerStyle>
                        </ListView>
                    </Grid>
                </Grid>
            </Border>
        </Grid>

        <!--Left Grid-->
        <Grid Name ="RightGrid" Grid.Column="2" Margin="10,20,20,20">
            <Grid.RowDefinitions>
                <RowDefinition Height="15*"/>
                <RowDefinition Height="70*"/>
                <RowDefinition Height="15*"/>
            </Grid.RowDefinitions>

            <!--Level info + uitleg Button-->
			<Grid Grid.Row="0" >
				<Grid.RowDefinitions>
                    <RowDefinition Height="{Binding ElementName=Coins, Path=Width}"/>
                </Grid.RowDefinitions>

				<Border Grid.Row="0" CornerRadius="5" >
					<Grid Grid.Row="0" >
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="65*"/>
							<ColumnDefinition Width="35*"/>
						</Grid.ColumnDefinitions>
                        <!--Level info-->
						<Grid Grid.Column="0" HorizontalAlignment="Center"  >
							<StackPanel VerticalAlignment="Center">
                                <TextBlock FontFamily="Tahoma" FontSize="20" FontWeight="Bold" Foreground="White" Text="{Binding CurrencyTray.CurrentStageString}"/>
                                <TextBlock FontFamily="Tahoma" FontSize="20" FontWeight="Bold" Foreground="White" Text="{Binding CurrencyTray.LevelProgressString}"/>
							</StackPanel>
						</Grid>
                        <!--Uitleg button-->
						<Grid Grid.Column="1" HorizontalAlignment="Right" Margin="0,0,10,0">
							<Button Name="switchscreen" MinWidth="130" Padding="20,20,20,20" BorderThickness="0" Click="UitlegScreen_OnClick">
								<Button.Background>
									<ImageBrush ImageSource="Assets/NHLStenden_startscherm.png"/>
								</Button.Background>
                                <TextBlock FontFamily="Tahoma" FontWeight="Bold" Foreground="White">Uitleg</TextBlock>
							</Button>
						</Grid>
					</Grid>
				</Border>
			</Grid>
            <!-- weergave level: Monster/Scene/coins/platform-->
			<Grid Grid.Row="1" Name="monster">
                <!--Scene-->
                <Image DataContext="{Binding CurrencyTray.CurrentScene.Background,Converter={StaticResource Base64ImageConverter}}" Source="{Binding Result}" Stretch="Fill" Margin="0,0,0,50"/>
                <Ellipse Name="platform" Fill="#E1E1E1"  Height="100" VerticalAlignment="Bottom" UseLayoutRounding="True" Stretch="Fill" StrokeDashCap="Round" StrokeLineJoin="Miter" StrokeStartLineCap="Flat"/>
				<!--Monster-->
                <Button Command="{Binding CurrencyTray.TappedEvent}" HorizontalAlignment="Center" VerticalAlignment="Bottom" Height="{Binding ElementName=monster, Path=Height}" Width="{Binding ElementName=monster, Path=Width}" Background="Transparent" Margin="75,100,75,35">
                    <Button.Resources>
                        <ResourceDictionary>
                            <SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="Transparent" />
                            <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="Transparent"/>
                            <SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="Transparent"/>
                            <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Transparent"/>
                        </ResourceDictionary>
                    </Button.Resources>
                    <Image HorizontalAlignment="Center" DataContext="{Binding CurrencyTray.CurrentMonster.Sprite,Converter={StaticResource Base64ImageConverter}}" Source="{Binding Result}"/>
                </Button>

                <!-- Coinsgrid-->
				<customui:CoinGrid VerticalAlignment="Bottom" Background="Transparent" Height="{Binding ElementName=platform, Path=Height}" Currencies="{Binding CurrencyTray.CurrencyInView}" Margin="20,0,20,5" Padding="0">
                    <customui:CoinGrid.RowDefinitions>
                        <RowDefinition Height="28*"/>
                        <RowDefinition Height="32*"/>
                        <RowDefinition Height="40*"/>
                    </customui:CoinGrid.RowDefinitions>
                    <customui:CoinGrid.ColumnDefinitions>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                        <ColumnDefinition Width="6.6*"/>
                    </customui:CoinGrid.ColumnDefinitions>
                </customui:CoinGrid>
            </Grid>
            
            <!--curentlevel info + progressbar-->
            <Grid Grid.Row="2">
                <Grid.RowDefinitions>
                    <RowDefinition Height="50*"/>
                    <RowDefinition Height="50*"/>
                </Grid.RowDefinitions>
                
                <!--Level Nummer-->
                <Grid Grid.Row="0" Margin="0,10,0,10">
                    <Grid HorizontalAlignment="center">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="45*"/>
                            <ColumnDefinition Width="10*"/>
                            <ColumnDefinition Width="45*"/>
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0">
                            <TextBlock VerticalAlignment="Center" HorizontalAlignment="Right" FontFamily="Tahoma" FontSize="30" FontWeight="Bold" Margin="0,0,10,0"  Foreground="White" Text="{Binding CurrencyTray.CurrentMonster.Name}"/>
                        </Grid>
                        <Grid Grid.Column="1">
                            <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Tahoma" FontSize="30" FontWeight="Bold" Margin="0,0,10,0" Foreground="White" >lvl.</TextBlock>
                        </Grid>
                        <Grid Grid.Column="2">
                            <TextBlock VerticalAlignment="Center" HorizontalAlignment="Left" FontFamily="Tahoma" FontSize="30" FontWeight="Bold" Foreground="White" Text="{Binding CurrencyTray.CurrentMonster.MonsterLevel}" />
                        </Grid>
                    </Grid>
                </Grid>
                <!--Progressbar-->
                <Grid Grid.Row="1" Margin="0,10,0,10">
                    <ProgressBar Height="25" Width="300" Value="{Binding CurrencyTray.MonsterHealthPercentage}" Foreground="Red"/>
                </Grid>
            </Grid>
        </Grid>

        <!-- Overlaypanel Friends-->
        <RelativePanel Name="panel" Grid.Column="2">
            <Popup Name="popup" IsOpen="false"  IsLightDismissEnabled="True" RelativePanel.AlignLeftWithPanel="True" HorizontalAlignment="Right">
                <Popup.ChildTransitions>
                    <TransitionCollection>
                        <PaneThemeTransition Edge="Right"/>
                    </TransitionCollection>
                </Popup.ChildTransitions>
                <Border CornerRadius="5,0,0,5" Visibility="Visible">
                    <Grid Background="#2E7988" Grid.Column="0" Width="{Binding ElementName=popup, Path=Width}" Height="{Binding ElementName=popup, Path=Height}">
                        <Grid Name="friendGrid" Width="400" HorizontalAlignment="Center">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30*"/>
                                <RowDefinition Height="20*"/>
                                <RowDefinition Height="50*"/>
                            </Grid.RowDefinitions>
                            <!--Jouw vrienden-->
                            <Grid Grid.Row="0" Margin="0,40,0,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="15*"/>
                                    <RowDefinition Height="85*"/>
                                </Grid.RowDefinitions>
                                <TextBlock Grid.Row="0" Margin="0,0,0,10" HorizontalAlignment="Left" FontFamily="Tahoma" FontSize="20" Foreground="White" >Vrieden:</TextBlock>
                                <ListView  Grid.Row="1" Name="Friends" ItemsSource="{Binding HeroList}" Width="{Binding ActualWidth, ElementName=friendGrid}" HorizontalAlignment="Center" Padding="0" Margin="0" SelectionMode="none">
                                    <ListView.ItemTemplate>
                                        <DataTemplate>
                                            <Border BorderThickness="0,0,0,1" BorderBrush="#2EAAB6" Width="{Binding ElementName=Friends, Path=Width}">
                                                <Grid Margin="5">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="50*"/>
                                                        <ColumnDefinition Width="50*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid Grid.Column="0">
                                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White">Jarno</TextBlock>
                                                    </Grid>
                                                    <Grid Grid.Column="1">
                                                        <Button Margin="0,0,20,0" HorizontalAlignment="Right" VerticalAlignment="Center" CornerRadius="5" Foreground="White">Invite</Button>
                                                    </Grid>
                                                </Grid>
                                            </Border>
                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                    <ListView.ItemContainerStyle>
                                        <Style TargetType="ListViewItem">
                                            <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
                                        </Style>
                                    </ListView.ItemContainerStyle>
                                </ListView>
                            </Grid>
                            
                            <!--Uitnodigingen die zijn onstvangen-->
                            <Grid Grid.Row="1" Margin="0,40,0,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25*"/>
                                    <RowDefinition Height="75*"/>
                                </Grid.RowDefinitions>
                                <TextBlock Grid.Row="0" Margin="0,0,0,10" HorizontalAlignment="Left" FontFamily="Tahoma" FontSize="20" Foreground="White">Uitnodigingen</TextBlock>
                                <ListView Grid.Row="1" ScrollViewer.VerticalScrollBarVisibility="Hidden"  ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                                    ScrollViewer.HorizontalScrollMode="Enabled"                  
                                    ScrollViewer.VerticalScrollMode="Enabled"
                                    Name="Invites" ItemsSource="{Binding HeroList}" Width="{Binding ActualWidth, ElementName=friendGrid}" HorizontalAlignment="Center" Padding="0" Margin="0" SelectionMode="none">
                                    <ListView.ItemTemplate>
                                        <DataTemplate>
                                            <Border BorderThickness="0,0,0,1" BorderBrush="#2EAAB6" Width="{Binding ElementName=Invites, Path=Width}">
                                                <Grid Margin="5">
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="50*"/>
                                                        <ColumnDefinition Width="50*"/>
                                                    </Grid.ColumnDefinitions>
                                                    <Grid Grid.Column="0">
                                                        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White">Jarno</TextBlock>
                                                    </Grid>
                                                    <Grid Grid.Column="1">
                                                        <Button Margin="0,0,20,0" HorizontalAlignment="Right" VerticalAlignment="Center" CornerRadius="5" Background="red" Foreground="White" >Accepteren</Button>
                                                        <Button Margin="0,0,20,0" HorizontalAlignment="Left" VerticalAlignment="Center" CornerRadius="5" Background="Orange" Foreground="White" >Weigeren</Button>
                                                    </Grid>
                                                </Grid>
                                            </Border>
                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                    <ListView.ItemContainerStyle>
                                        <Style TargetType="ListViewItem">
                                            <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
                                        </Style>
                                    </ListView.ItemContainerStyle>
                                </ListView>
                            </Grid>

                            <!--Vrienden zoeken/uitnodigingen-->
                            <Grid Grid.Row="2" Margin="0,40,0,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="28*"/>
                                    <RowDefinition Height="72*"/>
                                </Grid.RowDefinitions>
                                <StackPanel>
                                    <TextBlock Grid.Row="0" Margin="0,0,0,10" HorizontalAlignment="Left" FontFamily="Tahoma" FontSize="20" Foreground="White" >Vrieden toevoegen:</TextBlock>
                                    <TextBox Grid.Row="1" Margin="0,0,0,5"  Width="400" PlaceholderText="Voer gebruikersnaam in" Foreground="White" CornerRadius="5" VerticalAlignment="Center"/>
                                    <Button Grid.Row="2" Margin="0,0,0,5" HorizontalAlignment="left" CornerRadius="5" Foreground="White">Search</Button>
                                </StackPanel>
                                <Grid Grid.Row="3">
                                    <ListView Name="searchFriend" ItemsSource="{Binding HeroList}" Width="{Binding ActualWidth, ElementName=friendGrid}" HorizontalAlignment="Center" Padding="0" Margin="0" SelectionMode="none">
                                        <ListView.ItemTemplate>
                                            <DataTemplate>
                                                <Border BorderThickness="0,0,0,1" BorderBrush="#2EAAB6" Width="{Binding ElementName=searchFriend, Path=Width}">
                                                    <Grid Margin="5">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="50*"/>
                                                            <ColumnDefinition Width="50*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Grid Grid.Column="0">
                                                            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" >Jarno</TextBlock>
                                                        </Grid>
                                                        <Grid Grid.Column="1">
                                                            <Button Margin="0,0,20,0" HorizontalAlignment="Right" VerticalAlignment="Center" CornerRadius="5" Foreground="White" >Uitnodiging sturen</Button>
                                                        </Grid>
                                                    </Grid>
                                                </Border>
                                            </DataTemplate>
                                        </ListView.ItemTemplate>
                                        <ListView.ItemContainerStyle>
                                            <Style TargetType="ListViewItem">
                                                <Setter Property="HorizontalAlignment" Value="Stretch"></Setter>
                                            </Style>
                                        </ListView.ItemContainerStyle>
                                    </ListView>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>
                </Border>
            </Popup>
        </RelativePanel>

        <!-- Overlaypanel Uitlegscherm-->
        <RelativePanel Name="panelUitleg" Grid.Column="2">
            <Popup Name="popupUitleg" IsOpen="false"  IsLightDismissEnabled="True" RelativePanel.AlignLeftWithPanel="True" HorizontalAlignment="Right">
                <Popup.ChildTransitions>
                    <TransitionCollection>
                        <PaneThemeTransition Edge="Right"/>
                    </TransitionCollection>
                </Popup.ChildTransitions>
                <Border CornerRadius="5,0,0,5" Visibility="Visible">
                    <Grid Background="#2E7988" Grid.Column="0" Width="{Binding ElementName=popupUitleg, Path=Width}" Height="{Binding ElementName=popupUitleg, Path=Height}">
                        <Grid Name="UitegGrid" Width="400" HorizontalAlignment="Center">
                            <StackPanel Grid.Row="2" Margin="0,40,0,0">
                                <TextBlock Foreground="White">Uitleg</TextBlock>
                            </StackPanel>
                        </Grid>
                    </Grid>
                </Border>
            </Popup>
        </RelativePanel>
    </Grid>
</Page>
